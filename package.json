{
  "name": "type-explorer",
  "version": "0.0.2",
  "publisher": "sisisin",
  "engines": {
    "vscode": "^1.47.0"
  },
  "license": "MIT",
  "displayName": "type-explorer",
  "description": "Explorer for TypeScript's Type Aliases/interfaces",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "typescript",
    "type alias",
    "interface",
    "tree view"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sisisin/type-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/sisisin/type-explorer/issues"
  },
  "homepage": "https://github.com/sisisin/type-explorer#readme",
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onView:typeExplorerView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "typeExplorerView",
          "icon": "assets/vscode-icons/light/list-tree.svg",
          "title": "Type Explorer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "typeExplorerView",
        "contents": "focus on property in TypeScript file"
      }
    ],
    "views": {
      "typeExplorerView": [
        {
          "id": "typeExplorerView",
          "name": "",
          "icon": "assets/vscode-icons/light/list-tree.svg",
          "contextualTitle": "Type Explorer Title"
        }
      ],
      "explorer": [
        {
          "id": "typeExplorer",
          "name": "Type Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "typeExplorerView.refreshTypeScriptProject",
          "when": "view == typeExplorerView || view == typeExplorer",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "typeExplorerView.refreshTypeScriptProject",
        "title": "Refresh TypeScript Project",
        "icon": {
          "dark": "assets/vscode-icons/dark/sync.svg",
          "light": "assets/vscode-icons/light/sync.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint --ext js,ts .",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "jest": "jest",
    "test": "node ./out/test/runTest.js",
    "pub": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.8",
    "@types/mocha": "^8.0.1",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "jest": "^26.2.2",
    "mocha": "^8.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ts-morph": "^7.1.3"
  }
}
